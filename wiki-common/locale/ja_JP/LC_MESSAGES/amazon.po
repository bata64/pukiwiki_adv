msgid "You have not permission to create new page."
msgstr "あなたは、新しいページを作成できる権利を持っていません。"

#~ msgid "Author"
#~ msgstr "作者"

#~ msgid "Critic"
#~ msgstr "評者"

#~ msgid "Date"
#~ msgstr "日付"

#~ msgid "RECOMMENDATION"
#~ msgstr "お薦め対象"

#~ msgid "Impression"
#~ msgstr "感想"

#~ msgid ""
#~ "// First of all, please delete the full text when you stop this review, "
#~ "and push the [update] button on the page.\n"
